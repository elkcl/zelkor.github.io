<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:yandex="http://news.yandex.ru" xmlns:media="http://search.yahoo.com/mrss/" xmlns:turbo="http://turbo.yandex.ru" version="2.0" >
	<channel>
		{% for post in site.posts %}
	 <item turbo="true">
	 	<link>{{site.url}}{{post.url}}</link>
	 	<turbo:content>
	 		<![CDATA[
	 		<header>
	 			<h1>{{post.title}}</h1>
	 			</header>
	 			<p>{{post.meta}}</p>
	 		{{ post.content }}
	 		]>
	 	</turbo:content>
	 	</item>
	 	{% endfor %}
	 	</channel>
</rss>